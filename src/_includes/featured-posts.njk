<div class="wrp-cover">
  <!-- Post Thumbnail -->
  <div class="post-thumb">
    <a href="{{ post.url }}">
      <img src="{{ post.data.image }}" alt="{{ post.data.imageAlt }}" class="img-fluid">
    </a>
  </div>
  <!-- Post Title -->
  <div class="post-title">
    <a href="{{ post.url }}">{{ post.data.title }}</a>  
  </div>
</div>